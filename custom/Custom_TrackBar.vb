Imports System.Drawing.Drawing2D
Imports System.ComponentModel
Imports System.Drawing.Imaging


<DefaultEvent("ValueChanged")> Class CustomTrackBar


#Region "declare"
    Inherits customControl
#Region "Properties"
    Dim _Maximum As Integer = 100
    Public Property Maximum() As Integer
        Get
            Return _Maximum
        End Get
        Set(ByVal value As Integer)
            If value > 0 Then _Maximum = value
            If value < _Value Then _Value = value
            Invalidate()
        End Set
    End Property
    Event ClickedForward()
    Event ValueChanged()
    Public _Value As Integer = 0
    Public Property Value() As Integer
        Get
            Return _Value
        End Get
        Set(ByVal value As Integer)

            Select Case value
                Case Is = _Value
                    Exit Property
                Case Is < 0
                    _Value = 0
                Case Is > _Maximum
                    _Value = _Maximum
                Case Else
                    _Value = value
            End Select

            Invalidate()
            RaiseEvent ValueChanged()
        End Set
    End Property
    Dim hc As Color
    Dim im1, im2 As Bitmap
    <Category("Appearance")>
    Public Property HandleColor As Color
        Get
            Return hc
        End Get
        Set(value As Color)
            hc = value
            createimages()
            Invalidate()
        End Set
    End Property
#End Region
    Dim mdown As Boolean = False
    Dim y As Single = 57
    Dim hy! = 30
    Dim hh! = 20
#End Region



    Sub New()
        Me.SetStyle(ControlStyles.DoubleBuffer Or ControlStyles.AllPaintingInWmPaint Or ControlStyles.ResizeRedraw Or ControlStyles.UserPaint Or ControlStyles.Selectable Or ControlStyles.SupportsTransparentBackColor, True)
        DoubleBuffered = True
        LockHeight = 60
    End Sub
    Dim t% = 0
    Protected Overrides Sub painthook()

        calc()
        With G

            .Clear(Parent.BackColor)
            .SmoothingMode = SmoothingMode.HighQuality
            .InterpolationMode = InterpolationMode.HighQualityBicubic
            .CompositingQuality = 2

            Dim cw! = 25 + (Width - 50) * (Value / Maximum)                                                                                         'calculated width
            tp = New Pen(col(10, Color.White))
            .DrawLine(tp, 25, y, Width - 25, y)                                                                                                      'background
            tp = New Pen(ForeColor)
            .DrawLine(tp, 25, y, cw, y)                                                                                                              'fill
            tp = New Pen(col(70, Color.Black))
            .DrawLine(tp, 25, y + 1, Width - 25, y + 1)                                                                                              'shadow

            Static im As Bitmap

            'Handle

            If mdown = False Then im = im1 Else im = im2
            .DrawImage(im, New Rectangle(cw - 20 + (25 - hh), hy + (25 - hh), hh, hh))

        End With
    End Sub


#Region "other functions"
    Protected Overrides Sub OnMouseDown(ByVal e As System.Windows.Forms.MouseEventArgs)
        MyBase.OnMouseDown(e)
        Dim mp = New Rectangle(New Point(e.Location.X, e.Location.Y), New Size(20, 20))
        Dim Bar As Rectangle = New Rectangle(25 + (Width - 50) * (Value / Maximum) - 19, hy, 25, 25)
        If Bar.Contains(e.Location) Then
            t = 0
            mdown = True
        End If


        If e.Button = MouseButtons.Left Then


            Dim mousepos = Math.Min(Math.Max(e.X, 25), Width - 25) - 25
            Dim value = CInt(Me.Maximum * mousepos / (Width - 50))
            If value > Me.Value And value < Me.Maximum Then
                Me.Value = value
            Else
                Me.Value = value
            End If

        End If
        RaiseEvent ClickedForward()
        animating = True

    End Sub

    Protected Overrides Sub OnMouseUp(ByVal e As System.Windows.Forms.MouseEventArgs)
        MyBase.OnMouseUp(e)
        mdown = False
    End Sub

    Protected Overrides Sub OnMouseMove(ByVal e As System.Windows.Forms.MouseEventArgs)
        MyBase.OnMouseMove(e)
        If mdown Then
            Me.Value = Maximum * (Math.Max(25, Math.Min(e.X, Width - 25)) - 25) / (Width - 50)
            RaiseEvent ClickedForward()
        End If
    End Sub

    Protected Overrides Sub OnMouseLeave(ByVal e As System.EventArgs)
        MyBase.OnMouseLeave(e) : mdown = False
    End Sub


    Sub calc()
        If mdown = True Then
            If t < 350 Then
                t += 1
                y = GetValue(Height - 3, 3, t, 350, Type.Smootherstep, EasingMethods.Critical_Damping, 1)
                hy = GetValue(30, 6, t, 350, Type.SmoothStep, 1)
                hh = GetValue(20, 25, t, 350, Type.SmoothStep, 1)

            End If
        Else
            If t > 0 Then
                t -= 1
                y = GetValue(3, Height - 3, 350 - t, 350, Type.Smootherstep, EasingMethods.Critical_Damping, 1)
                hy = GetValue(30, 6, t, 350, Type.SmoothStep, 1)
                hh = GetValue(20, 25, t, 350, Type.SmoothStep, 1)

            Else : animating = 0
            End If
        End If
        Debug.WriteLine(hy)
    End Sub

    Protected Overrides Sub OnCreateControl()
        MyBase.OnCreateControl() : createimages()
    End Sub
    Sub createimages()
        im1 = New Bitmap(Image.FromStream(New System.IO.MemoryStream(Convert.FromBase64String(""))))
        im2 = New Bitmap(Image.FromStream(New System.IO.MemoryStream(Convert.FromBase64String(""))))

        Graphics.FromImage(im1).SmoothingMode = 2
        Graphics.FromImage(im1).InterpolationMode = 7
        Graphics.FromImage(im1).CompositingQuality = 2
        Graphics.FromImage(im2).SmoothingMode = 2
        Graphics.FromImage(im2).InterpolationMode = 7
        Graphics.FromImage(im2).CompositingQuality = 2

        For i = 0 To 19
            For j = 0 To 19
                im1.SetPixel(i, j, Color.FromArgb(im1.GetPixel(i, j).R, hc))
                im2.SetPixel(i, j, Color.FromArgb(im2.GetPixel(i, j).R, hc))
            Next
        Next
    End Sub
#End Region
End Class 'DISPOSE DONE
